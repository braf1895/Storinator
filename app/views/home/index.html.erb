<h1>Welcome, <%= session['user_fName']%>!</h1>
<script type='text/javascript'>
  function isAdmin(){
    var ad = <%= session['user_isAdmin'] %>;
    console.log(ad);
    if(ad === 0){
      document.getElementById('admins').style.display="none";
    }
  };
  isAdmin();
</script>
<h2 id=”notice”><%= notice %></h2>
<h2>Full View</h2>
<ul class="list-group">
  <li class='list-group-item'></li>
